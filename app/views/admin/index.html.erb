<div id="user_list">
	<h2> List of Users</h2>
	<table id="user_table">
		<tr>
			<td id="label">User Name</td>
			<td id="label">Email</td>
			<td id="label">Employee Id</td>
			<td id="label">Role</td>
		</tr>	
	<% for user in @users do %>
	<tr>
		<td><%= user.full_name %></td>
		<td><%= user.email %></td>
		<td><%= user.employee_id %></td>
		<td><%= user.role %></td>
	</tr>
	<% end %>
	</table>
</div>

<div id="member_list">
	<h2> List of Members</h2>
	<table id="member_table">
		<tr>
			<td id="label">Member Name</td>
			<td id="label">Email</td>
			<td id="label">Phone No</td>
		</tr>	
	<% for member in @members do %>
	<tr>
		<td><%= member.first_name %> <%= member.last_name %> </td>
		<td><%= member.email %></td>
		<td><%= member.phone_number %></td>
	</tr>
	<% end %>
	</table>
</div>
